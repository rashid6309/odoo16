<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ec_medical_labs_tree_view" model="ir.ui.view">
            <field name="name">ec.medical.labs.tree.view</field>
            <field name="model">ec.medical.labs</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ec_medical_labs_action" model="ir.actions.act_window">
            <field name="name">Labs</field>
            <field name="res_model">ec.medical.labs</field>
            <field name="view_mode">tree,form</field>
            <field name="target">current</field>
        </record>

        <!-- Menu-item for Configuration -->

        <menuitem
            id="ec_medical_labs_menu"
            name="Labs"
            action="ec_medical_labs_action"
            parent="ec_medical_history_configuration"
            sequence="4900"/>

    </data>
</odoo>