<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_ec_medical_profession_tree" model="ir.ui.view">
            <field name="name">ec.medical.profession.tree</field>
            <field name="model">ec.medical.profession</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ec_medical_profession_action" model="ir.actions.act_window">
            <field name="name">Medical Professions</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ec.medical.profession</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="ec_medical_profession_menu"
            name="Medical Professions"
            action="ec_medical_profession_action"
            parent="ec_medical_history_configuration"
            sequence="5300"/>

    </data>
</odoo>