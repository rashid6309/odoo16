								<div class="col-6 text-right" style="margin-top:9px"/>
							</div>

							<div class="card-block mb-5 pl-4 collapse" id="sx_contraception_female_section">
								<div>
									<h2>
										Sexual
									</h2>
								</div>
								<br/>
								<group>
									<field name="sx_frequency"/>
									<field name="sx_issues" widget="radio" options="{'horizontal': True}"/>
									<field name="sx_comments"/>
								</group>

								<div>
									<h2> Contraception</h2>
								</div>
								<br/>
