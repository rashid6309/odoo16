		<record id="ec_medical_first_consultation_form_view" model="ir.ui.view">
			<field name="name">ec.first.consultation.form</field>
			<field name="model">ec.first.consultation</field>
			<field name="arch" type="xml">
				<form string="First Consultation">
					<notebook>
						<page string="Female" name="female">
							<div class="d-flex scroll-to card-header" id="general_examination_section_link">
								<div class="col-6">
									<a data-bs-toggle="collapse" data-bs-target="#general_examination_section" href="#"><h4 class="mt-2 mb-0">General Examination</h4></a>
								</div>
								<div class="col-6 text-right" style="margin-top:9px">
								</div>
							</div>
							<div class="card-block mb-5 pl-4 collapse" id="general_examination_section">
								<group>
									<!-- General Examination Fields -->
									<field name="name"/>
									<field name="female_comment"/>
									<field name="female_age"/>
									<field name="female_marriage" widget="radio" options="{'horizontal': True}"/>
									<field name="female_relation" widget="radio" options="{'horizontal': True}"/>
									<field name="female_family_system"/>
									<field name="female_living_togather"/>
									<field name="female_living_togather_reason"/>
									<field name="female_living_duration_years"/>
									<field name="female_living_duration_month"/>
									<field name="female_complaints"/>
									<field name="female_complaints_duration_years"/>
									<field name="female_complaints_duration_month"/>
									<field name="female_age_at_marriage"/>
									<field name="female_parity"/>
								</group>
							</div>

							<div class="d-flex scroll-to card-header" id="obstetrics_history_section_link">
								<div class="col-6">
									<a data-bs-toggle="collapse" data-bs-target="#obstetrics_history_section" href="#"><h4 class="mt-2 mb-0">Obstetrics History</h4></a>
								</div>
								<div class="col-6 text-right" style="margin-top:9px">
								</div>
							</div>
							<div class="card-block mb-5 pl-4 collapse" id="obstetrics_history_section">
								<tree editable="top">
									<field name="obs_history_ids"
										   nolabel="1" editable="top">
									</field>
								</tree>
							</div>


							<div class="d-flex scroll-to card-header" id="gynaecological_history_section_link">
								<div class="col-6">
									<a data-bs-toggle="collapse" data-bs-target="#gynaecological_history_section" href="#"><h4 class="mt-2 mb-0">Gynaecological History</h4></a>
								</div>
								<div class="col-6 text-right" style="margin-top:9px">
								</div>
							</div>
							<div class="card-block mb-5 pl-4 collapse" id="gynaecological_history_section">
								<group>
									<!-- Gynaecological History Fields -->
									<field name="female_age_at_menarche"/>
									<field name="female_menarche_type"/>
									<field name="female_menstrual_cycle"/>
									<field name="female_menstrual_days"/>
