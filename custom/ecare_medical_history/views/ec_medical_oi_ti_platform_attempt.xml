                                        <field name="tvs_hyperechoic_solid"/>
                                        <span for="tvs_smooth" style=" margin-right: 2%; ">Hyperechoic/Solid</span>
                                        <div class="d-flex">
                                            <field name="tvs_suspected_cavity_lesion"/>
                                            <span for="tvs_smooth" style=" margin-right: 2%; ">Suspected Cavity Lesion</span>
                                            <field name="tvs_menstruating"/>
                                            <span for="tvs_menstruating" style=" margin-right: 2%; ">Menstruating</span>
                                        </div>

                                    </div>
                                    <div>
                                        <span>CET</span>
                                        <field name="tvs_lining_size_decimal" style="width:12% !important;" class="oe_inline" nolabel="1"/>
                                        <span>mm</span>
                                    </div>
                                </div>
                            </div>

                            <group string="Ovaries"/>
                            <span class="bg-light bg-gradient" style=" margin-left: 1.5rem; ">
                                <div class="d-flex justify-content-start bg-light bg-gradient mb-2">
                                    <div style="text-align: end;margin-right: 2rem;">
                                        ROV
                                    </div>

                                    <div class="col-md-5" style="margin-right: 1rem;">
