                                            <field name="tvs_rov"/>

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <a class="btn btn-primary oe_edit_only" onclick="showBeginScan('rov', 'tvs_rov')">Begin Scan</a>

                                        <div style="display: inline; margin-left: 2%;">
