<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!-- Explicit Form View -->

        <record id="view_ec_medical_oi_ti_platform_form" model="ir.ui.view">
            <field name="name">ec.medical.oi.ti.platform.form</field>
            <field name="model">ec.medical.oi.ti.platform</field>
            <field name="arch" type="xml">
                <form>
                    <sheet class="container">
                        <group>
                            <field name="timeline_id"/>
                            <field name="repeat_consultation_id"/>
                            <field name="oi_ti_diagnosis_ids"
                                   widget="many2many_tags"
                                   options="{'no_create': True, 'no_open': True}"/>
                            <field name="oi_ti_other_diagnosis"/>
                            <field name="oi_ti_follicle_left"/>
                            <field name="oi_ti_follicle_right"/>
                            <field name="timeline_id"/>
                        </group>
                    </sheet>
                </form>

            </field>
        </record>



        <!-- Explicit Kanban View -->

        <record id="view_ec_medical_oi_ti_platform_kanban" model="ir.ui.view">
            <field name="name">ec.medical.oi.ti.platform.kanban</field>
            <field name="model">ec.medical.oi.ti.platform</field>
            <field name="arch" type="xml">
                <kanban  class="max_width_kanban">
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_global_click">
                                <table cellspacing="0" cellpadding="0" style="width: 100%;; border:0.75pt solid #cccccc; border-collapse:collapse;">
                                    <tbody>
                                        <tr style="height:15.75pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                            <td style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#cccccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"></p>
                                            </td>
                                        </tr>
                                        <tr style="height:15.75pt;">
                                            <td colspan="5" style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#ff00ff;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong><span style="color:#ffffff;">OUTCOME: PREGNANCY (+VE UPT or BHCG)</span></strong></p>
                                            </td>
                                            <td colspan="2" style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#666666;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">ATTEMPT COMPLETED</span></strong></p>
                                            </td>
                                        </tr>
                                        <tr style="height:15.75pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong><span style="color:#ffffff;">Trigger regimen</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong>IVF-C 10,000IU S/C</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">Insemination</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong>Fresh IUI</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">IUI attempt</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">Luteal phase support</span></strong></p>
                                            </td>
                                            <td style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">2nd Trigger regimen</span></strong></p>
                                            </td>
                                        </tr>
                                        <tr style="height:26.25pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong><span style="color:#ffffff;">Trigger date &amp; time</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; text-align:center; line-height:115%; widows:0; orphans:0; font-size:8pt;"><span style="color:#ffffff;">(hours and min, NO seconds)</span></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">Indication for IUI</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong>Erectile dysfunction</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">I 2 3</span></strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong>C + D2</strong></p>
                                            </td>
                                            <td style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#0b5394;">
                                                <p class="oi_ti_action_btn"><strong><span style="color:#ffffff;">2nd Trigger date &amp; time</span></strong></p>
                                            </td>
                                        </tr>
                                        <tr style="height:15.75pt;">
                                            <td colspan="7" style="border-top:0.75pt solid #cccccc; border-right-style:solid; border-right-width:0.75pt; border-bottom:0.75pt solid #cccccc; padding-top:2pt; padding-bottom:2pt; vertical-align:top; background-color:#f4cccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong><span style="color:#1f1f1f;">OI with clomiphene citrate, 50mg OD, from cycle days 2 to 6 (carry forward previous value)</span></strong></p>
                                            </td>

                                        </tr>
                                        <tr style="height:15.75pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#f4cccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">OI day: 3</p>
                                            </td>
                                            <td colspan="2" style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#fff2cc;">
                                                <p class="oi_ti_action_btn"><strong>LEFT OVARY</strong></p>
                                            </td>
                                            <td colspan="2" style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#d9ead3;">
                                                <p class="oi_ti_action_btn"><strong>RIGHT OVARY</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#ffffff;">
                                                <p class="oi_ti_action_btn"><strong>Signs of ovulation</strong></p>
                                            </td>
                                            <td style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#f4cccc;">
                                                <p class="oi_ti_action_btn"><strong>Diagnosis</strong></p>
                                            </td>
                                        </tr>
                                        <tr style="height:26.25pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#f4cccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">LMP</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#fff2cc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">Follicle(s)</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#fff2cc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">(<field name="oi_ti_follicle_left"/>)</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#d9ead3;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">Follicle(s)</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#d9ead3;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">(<field name="oi_ti_follicle_left"/>)</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#ffffff;">
                                                <p class="oi_ti_action_btn">Free fluid, loss of dominant follicle, corpus luteum</p>
                                            </td>
                                            <td style="border-top:0.75pt solid #cccccc; border-left:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#f4cccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><field name="oi_ti_diagnosis_ids"/></p>
                                            </td>
                                        </tr>
                                        <tr style="height:15.75pt;">
                                            <td style="border-top:0.75pt solid #cccccc; border-right:0.75pt solid #cccccc; border-bottom:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#f4cccc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong>Cycle day:</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#fff2cc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong>Dominant Follicle(s)</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#fff2cc;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">None</p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#d9ead3;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;"><strong>Dominant Follicle(s)</strong></p>
                                            </td>
                                            <td style="border:0.75pt solid #cccccc; padding:2pt 1.62pt; vertical-align:top; background-color:#d9ead3;">
                                                <p style="margin-top:0pt; margin-bottom:0pt; line-height:115%; widows:0; orphans:0; font-size:10pt;">None</p>
