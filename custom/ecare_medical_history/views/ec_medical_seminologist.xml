<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_ec_medical_seminologist_tree" model="ir.ui.view">
            <field name="name">ec.medical.seminologist.tree</field>
            <field name="model">ec.medical.seminologist</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="user_id"/>
                </tree>
            </field>
        </record>

        <record id="ec_medical_seminologist_action" model="ir.actions.act_window">
            <field name="name">Medical Seminologist</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ec.medical.seminologist</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="ec_medical_seminologist_menu"
            name="Medical Seminologist"
            action="ec_medical_seminologist_action"
            parent="ec_medical_history_configuration"
            sequence="5600"/>

    </data>
</odoo>