<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ec_medical_investigation_tree_view" model="ir.ui.view">
            <field name="name">ec.medical.investigation.tree.view</field>
            <field name="model">ec.medical.investigation</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ec_medical_investigation_action" model="ir.actions.act_window">
            <field name="name">Investigations</field>
            <field name="res_model">ec.medical.investigation</field>
            <field name="view_mode">tree</field>
            <field name="target">current</field>
        </record>


        <!-- Menu-item for Configuration -->

        <menuitem
            id="ec_medical_investigation_menu"
            name="Investigations"
            action="ec_medical_investigation_action"
            parent="ec_medical_history_configuration"
            sequence="4700"/>

    </data>
</odoo>