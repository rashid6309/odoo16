<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ec_medical_obstetrics_history_tree_view" model="ir.ui.view">
            <field name="name">ec.obstetrics.history.tree</field>
            <field name="model">ec.obstetrics.history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="patient_id"
                           readonly="1"/>

                    <field name="first_consultation_id"/>

                    <field name="baby_name"/>
                    <field name="pob"/>
                    <field name="dob"/>
                    <field name="age"/>
                    <field name="baby_notes"/>
                    <field name="dop"/>
                    <field name="mod"/>
                    <!--                    <field name="indication"/>-->
                    <field name="other_indications"/>
                    <field name="complications"/>
                    <field name="comp_del"/>
                    <field name="comp_pp"/>
                    <field name="comp_prag"/>
                    <field name="gender"/>
                    <field name="weight"/>
                    <field name="health"/>
                    <field name="alive"/>
                    <field name="feed"/>
                    <field name="state"/>
                    <field name="labour_history"/>
                    <field name="legacy_system_ID"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>