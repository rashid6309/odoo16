<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ec_medical_obstetrics_history_tree_view" model="ir.ui.view">
            <field name="name">ec.obstetrics.history.tree</field>
            <field name="model">ec.obstetrics.history</field>
            <field name="arch" type="xml">
                <tree editable="top" export_xlsx="false">
                    <field name="patient_id"
                           invisible="1"
                           readonly="1"
                           force_save="1"/>
                    <field name="create_date"/>
                    <field name="baby_name"/>
                    <field name="mode_of_delivery" required="1"/>
                    <field name="duration_of_pregnancy" string="Dur. of Preg"/>
                    <field name="year" options="{'no_open': True, 'no_create': True}"/>
                    <field name="date_of_birth"/>
                    <field name="age"/>
                    <field name="birth_place"/>
                    <field name="complication_pregnancy"/>
                    <field name="labour_history"/>
<!--                    <field name="baby_notes"/>-->
<!--                    <field name="complications"/>-->
                    <field name="complication_delivery"/>
                    <field name="complication_post_partum"/>
                    <field name="gender"/>
                    <field name="weight"/>
                    <field name="health"/>
                    <field name="child_health_issues"/>
                    <field name="alive"/>
                    <field name="feed"/>
                    <field name="other_indications"/>
<!--                    <field name="legacy_system_ID"/>-->
                </tree>
            </field>
        </record>

    </data>
</odoo>