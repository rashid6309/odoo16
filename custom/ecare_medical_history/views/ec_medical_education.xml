<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_ec_medical_education_tree" model="ir.ui.view">
            <field name="name">ec.medical.education.tree</field>
            <field name="model">ec.medical.education</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ec_medical_education_action" model="ir.actions.act_window">
            <field name="name">Medical Educations</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ec.medical.education</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="ec_medical_education_menu"
            name="Medical Educations"
            action="ec_medical_education_action"
            parent="ec_medical_history_configuration"
            sequence="5400"/>

    </data>
</odoo>