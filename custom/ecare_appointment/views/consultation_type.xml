<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!-- Explicit Tree view -->
        <record id="consultation_type_tree_view" model="ir.ui.view">
            <field name="name">consultation.type.tree.view</field>
            <field name="model">consultation.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <!-- Explicit form view -->
        <record id="consultation_type_form_view" model="ir.ui.view">
            <field name="name">consultation.type.form.view</field>
            <field name="model">consultation.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>

        </record>

        <!-- Explicit action -->
        <record id="consultation_type_action" model="ir.actions.act_window">
            <field name="name">Consultation Type</field>
            <field name="res_model">consultation.type</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>