<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!-- Explicit Tree View -->
        <record id="schedule_tree_view" model="ir.ui.view">
            <field name="name">schedule.tree.view</field>
            <field name="model">ec.slot.schedule</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="day"/>
                    <field name="active_time"/>
                    <field name="to_time"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>